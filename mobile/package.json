    "scripts": {
        "lint": "tslint \"src/**/*.ts\"",
        "ngcc": "ngcc --properties es2015 module main --first-only",
        "postinstall": "npm run ngcc",
        "crazy-clean": "npx rimraf hooks node_modules platforms package-lock.json && npm i",
